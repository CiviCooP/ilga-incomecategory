<?xml version="1.0"?>
<extension key="ilga-incomecategory" type="module">
  <file>ilga_incomecategory</file>
  <name>ILGA Income Category</name>
  <description>Adds a job to categorize an organization in a income category, according to the worldbank table, looking
               at the active address.
  </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Klaas Eikelboom</author>
    <email>klaas.eikelboom@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/CiviCooP/ilga-incomecategory</url>
    <url desc="Documentation">https://github.com/CiviCooP/ilga-incomecategory/blob/master/README.md</url>
    <url desc="Support">helpdesk@civicoop.org</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-01-25</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new module, tested on one instance</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/IlgaIncomecategory</namespace>
  </civix>
</extension>
